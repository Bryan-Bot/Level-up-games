<h1>Sign Up</h1>

<%= form_for @user do |f| %>
    <%= f.label :user_name %></br>
    <%= f.text_field :user_name %>
    <br>
    <%= f.label :email_address %>
    <%= f.text_field :email_address %>
    <br>
    <%= f.label :password%>
    <%= f.password_field :password%>

    <%= f.submit  %>
<% end %>

<hr>
<%= link_to "home", games_path%>